package cdac.b1.core;

import java.time.LocalDate;

public class Contact {
	private String pNo;
	private String name;
	private LocalDate dob;
	private String email;
	
	public Contact(String pNo, String name, LocalDate dob, String email) {
		super();
		this.pNo = pNo;
		this.name = name;
		this.dob = dob;
		this.email = "";
	}
	
	
}
