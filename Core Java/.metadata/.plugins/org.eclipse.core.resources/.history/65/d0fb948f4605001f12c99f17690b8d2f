package cdac.java.q6.utils;

import java.util.HashMap;
import java.util.Map;

import cdac.java.q6.employee.Employee;

public class EmpUtils {
	public static Map<Integer, Employee> populateMap(){
		Map<Integer, Employee> employees = new HashMap<>();
		employees.put(1, new Employee("John Doe", LocalDate.of(2020, 5, 15), "1234567890", "123456789012", 50000.0));
        employees.put(2, new Employee("Jane Smith", LocalDate.of(2019, 10, 20), "9876543210", "987654321098", 60000.0));
        employees.put(3, new Employee("Alice Johnson", LocalDate.of(2021, 3, 8), "5555555555", "555555555555", 55000.0));
        employees.put(4, new Employee("Bob Williams", LocalDate.of(2018, 7, 3), "9999999999", "999999999999", 70000.0));
		
	}
}
